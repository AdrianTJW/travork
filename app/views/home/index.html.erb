<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if current_user %>
	<%= link_to "View jobs", jobs_path %>
	<%= link_to "Log out - User", destroy_user_session_path ,method: :delete %>
<% elsif current_host %>
	<% if current_host.verify_status == true %>
		<%= link_to "Host jobs", new_job_path %>
	<% end %>
	<%= link_to "View jobs", jobs_path %>
	<%= link_to "Log out - Host", destroy_host_session_path ,method: :delete %>
<% else %>
	<%= link_to "New - User", new_user_registration_path %>
	<%= link_to "New - Host", new_host_registration_path %>
	<%= link_to "Sign in - User", new_user_session_path %>
	<%= link_to "Sign in - Host", new_host_session_path %>
	<%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook) %>
	<%= link_to "Sign in with Google", user_omniauth_authorize_path(:google_oauth2) %>
<% end %>

<%= link_to "New admin", new_admin_registration_path %>
